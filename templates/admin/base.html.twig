<!DOCTYPE html>
<html lang="{{ app.request.locale }}" class="no-js">
<head>
    <script>document.querySelector('html').classList.remove('no-js');</script>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>{% block title %}Tickets - Admin Panel{% endblock %}</title>
    <link rel="icon" href="{{ asset('favicon.svg') }}">
    {% block stylesheets %}{% endblock %}
</head>
<body>
    {{ knp_menu_render('app_admin_menu', {
        'template': 'admin/menu/template.html.twig',
        'currentClass': 'active',
        'ancestorClass': 'active open',
        'compressed': true
    }) }}
    {{ knp_menu_render('app_admin_menu', {
        'template': 'admin/menu/breadcrumb.html.twig',
        'compressed': true
    }) }}
    {% block body %}{% endblock %}
    {% block javascripts %}{% endblock %}
</body>
</html>
